{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=sms_database.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MessageHub": "Debug",
      "Inetlab.SMPP": "Debug"
    }
  },
  "SmppSettings": {
    "Host": "smscsim.smpp.org",
    "Port": 2775,
    "SystemId": "YOUR_SMPP_USERNAME",
    "Password": "YOUR_SMPP_PASSWORD",
    "MaxConnections": 3
  },
  "HttpSmsChannel": {
    "ProviderName": "Twilio",
    "ApiUrl": "https://api.twilio.com/2010-04-01/Accounts/YOUR_ACCOUNT_SID/Messages.json",
    "ApiKey": "YOUR_TWILIO_AUTH_TOKEN",
    "AuthorizationType": "Basic",
    "FromNumber": "YOUR_TWILIO_PHONE_NUMBER",
    "TimeoutMs": 10000
  },
  "KeyVaultEndpoint": "https://YOUR_KEYVAULT_NAME.vault.azure.net/",
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  }
}